<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white3"
    android:paddingTop="?android:attr/actionBarSize"
    android:paddingHorizontal="24dp"
    tools:context=".FinishActivity">

    <TextView
        android:id="@+id/finishTextMessage"
        style="@style/Theme.Latihan3activitymonitoring.Title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/finish_text_message"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <android.widget.Button
        android:id="@+id/btnOk"
        android:onClick="actFinish"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="62dp"
        android:layout_marginVertical="20dp"
        style="@style/Theme.Monitory.Button"
        android:background="@drawable/btn_primary"
        android:text="@string/oke"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>